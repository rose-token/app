# Server Configuration
PORT=3000

# GitHub Integration
# Create a personal access token at: https://github.com/settings/tokens
# Required scopes: repo (full control of private repositories)
GITHUB_TOKEN=ghp_your_github_personal_access_token_here

# Marketplace Contract Address (OPTIONAL)
# The server will automatically read this from deployment-output.json if not set
# Only set this manually if you want to override the deployment artifact
# MARKETPLACE_ADDRESS=0x...

# Alchemy Webhook (optional)
# Used for verifying webhook signatures (if Alchemy provides signing)
ALCHEMY_WEBHOOK_SECRET=your_alchemy_webhook_secret_here

# Smart Contract Deployment (for deployment scripts)
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID
PRIVATE_KEY=your_wallet_private_key_without_0x_prefix
DAO_TREASURY_ADDRESS=0x_treasury_address_for_dao_funds

# Etherscan Verification
ETHERSCAN_API_KEY=your_etherscan_api_key_for_verification
